section {
    height: 700px;
}

#submenu {
    float: left;
    height: inherit;
    border-right: 1px solid #333;
    width: 200px;
}

#submenu ul{
    list-style: none;
    margin: 0;
    padding: 0;
}

#submenu ul li {
    padding: 8px;
    border-bottom: 1px solid #333;
}

#submenu ul li {
    color: #333;
    font-size: 20px;
    text-decoration: none;
}

.current_submenu_item a,
#submenu ul li:hover {
    font-size: 20px;
    text-decoration: none;
    background-color: rgba(51, 153, 34, 0.25);
}

#submenu ul li:active {
    font-size: 20px;
    text-decoration: none;
    background-color: rgba(51, 153, 34, 0.5);
}

.current_submenu_item a {
    color: #ff7;
}

#submenu ul a {
    text-decoration: none;
}

.content{
    padding: 10px;
    margin: 10px;
    float: left;
    height: 550px;
    width: 830px;

    display:block;
}

#middenmeer_img{
    background-image:url('../_pictures/parken/middenmeer.png');
    width: 850px;
    height: 500px;
}

#ijburg_img{
    background-image:url('../_pictures/parken/ijburg_small.png');
    width: 535px;
    height: 600px;
}

#drieburg_img{
    background-image:url('../_pictures/parken/drieburg_small.png');
    width: 850px;
    height: 500px;
}

#voorland_img{
    background-image:url('../_pictures/parken/voorland_small.png');
    width: 640px;
    height: 589px;
}
#field_popup_link{

    border: thin black solid;
    background: rgba(255, 255, 255, 0.1);

    font-size: 9px;

    float:right;
    vertical-align: bottom;

    padding: 3px;
    margin: 5px;
}
.field_popup_box{
    display: none;
    width: 150px;
    height: 50px;
    position: absolute;
    border: black thin solid;

}


.field{
    border: thin white solid;
    background-color: rgba(10, 255, 10, 0.3);
    position: absolute;

    -moz-user-select: none;
    -webkit-user-select: none;

    height: 105px;
    width: 75px;
}

