section {
	height: 600px;
}
#submenu {
    float: left;
    height: inherit;
    border-right: 1px solid #333;
    width: 200px;
}

#submenu ul{
    list-style: none;
    margin: 0;
    padding: 0;
}

#submenu ul li {
    padding: 8px;
    border-bottom: 1px solid #333;
}

#submenu ul li {
    color: #333;
    font-size: 20px;
    text-decoration: none;
}

.current_submenu_item a,
#submenu ul li:hover {
    font-size: 20px;
    text-decoration: none;
    background-color: rgba(51, 153, 34, 0.25);
}

#submenu ul li:active {
    font-size: 20px;
    text-decoration: none;
    background-color: rgba(51, 153, 34, 0.5);
}

.current_submenu_item a {
    color: #ff7;
}

#submenu ul a {
    text-decoration: none;
}
